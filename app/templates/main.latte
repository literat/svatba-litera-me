						</tr>
						<tr>
							<td>pro grafiky:</td>
							<td>PANTONE2767C</td>
							<td>PANTONE188XGC</td>
							<td>PANTONE922OU</td>
							<td>PANTONE10102C</td>
						</tr>
						<tr>
							<td>pro muže:</td>
							<td>modrá</td>
							<td>červená</td>
							<td>bez názvu</td>
